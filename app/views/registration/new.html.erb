<h1>Bienvenue !</h1>

<a href="/registration/login">S'identifier</a>
<%= form_for @user, url: {action: "create"} do |f| %>
  <%= f.text_field :name, autofocus: true, placeholder: 'Name' %> <br><br><br>
  <%= f.text_field :email, autofocus: true, placeholder: 'Example@ex.com' %> <br><br><br>
  <%= f.text_field :password, autofocus: true, placeholder: 'password'  %>
  <%= f.submit "Create" %>
<% end %>